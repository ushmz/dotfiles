{
  "maxNumberOfProblems": 100,
  "numSuggestions": 10,
  "allowCompoundWords": true,
  "dictionaryDefinitions": [
    {
      "name": "dotfiles",
      "path": "dotfiles.txt",
      "description": "A list of words that are not in the dictionary but are commonly used in dotfiles.",
      "addWords": true
    },
    {
      "name": "user",
      "path": "user.txt",
      "description": "A list of words that are not in the dictionary but should not staged for git",
      "addWords": true
    }
  ],
  "words": ["fmtag"],
  "import": [
    "@cspell/dict-bash/cspell-ext.json",
    "@cspell/dict-css/cspell-ext.json",
    "@cspell/dict-docker/cspell-ext.json",
    "@cspell/dict-en-common-misspellings/cspell-ext.json",
    "@cspell/dict-en_us/cspell-ext.json",
    "@cspell/dict-filetypes/cspell-ext.json",
    "@cspell/dict-git/cspell-ext.json",
    "@cspell/dict-golang/cspell-ext.json",
    "@cspell/dict-google/cspell-ext.json",
    "@cspell/dict-html/cspell-ext.json",
    "@cspell/dict-lua/cspell-ext.json",
    "@cspell/dict-markdown/cspell-ext.json",
    "@cspell/dict-node/cspell-ext.json",
    "@cspell/dict-python/cspell-ext.json",
    "@cspell/dict-rust/cspell-ext.json",
    "@cspell/dict-software-terms/cspell-ext.json",
    "@cspell/dict-sql/cspell-ext.json",
    "@cspell/dict-terraform/cspell-ext.json",
    "@cspell/dict-typescript/cspell-ext.json"
  ],
  "dictionaries": ["dotfiles", "user", "google", "vim"],
  "patterns": [
    {
      "name": "Keymap description",
      "pattern": "[a-z]*\\[[A-Z]\\][a-z]+",
      "description": "Emphasize key used in keymap"
    }
  ],
  "ignoreRegExpList": ["Keymap description"],
  "languageSettings": [
    {
      "dictionaries": ["lua"],
      "locale": "*",
      "languageId": "lua",
      "ignoreRegExpList": ["/require.*/"]
    }
  ]
}
