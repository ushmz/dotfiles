[
    {
        "key": "cmd+shift+t",   "command": "workbench.action.terminal.focus",
                                "when": "editorTextFocus"
    },
    {
        "key": "cmd+[",         "command": "workbench.action.terminal.focusPrevious",
                                "when": "terminalFocus"
    },
    {
        "key": "cmd+]",         "command": "workbench.action.terminal.focusNext",
                                "when": "terminalFocus"
    },
    {
        "key": "tab",           "command": "selectNextSuggestion",
                                "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
    },
    {
        "key": "shift+tab",     "command": "selectPrevSuggestion",
                                "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
    }
]
