[
  {
    "command": "vscode-neovim.compositeEscape1",
    "key": "j",
    "when": "neovim.mode == insert && editorTextFocus",
    "args": "j"
  },
  {
    "key": "space w",
    "command": "workbench.action.files.save",
    "when": "neovim.mode == normal && editorFocus"
  },
  {
    "key": "space q",
    "command": "workbench.action.closeActiveEditor",
    "when": "neovim.mode == normal && editorFocus"
  },
  {
    "key": "space f",
    "command": "editor.action.formatDocument",
    "when": "neovim.mode == normal && editorFocus"
  },
  {
    "key": "u",
    "command": "undo",
    "when": "neovim.mode != 'insert' && editorFocus"
  },
  {
    "key": "ctrl+r",
    "command": "redo",
    "when": "neovim.mode != 'insert' && editorFocus"
  },
  {
    "key": "space v",
    "command": "workbench.action.splitEditor",
    "when": "neovim.mode == normal && editorFocus"
  },
  {
    "key": "space s",
    "command": "workbench.action.splitEditorDown",
    "when": "neovim.mode == normal && editorFocus"
  },
  {
    "key": "cmd+shift+t",
    "command": "workbench.action.terminal.focus",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+t [",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+t ]",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+t h",
    "command": "workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+t l",
    "command": "workbench.action.terminal.focusNextPane",
    "when": "terminalFocus"
  },
  {
    "key": "tab",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "shift+tab",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+p",
    "command": "cursorUp",
    "when": "editorTextFocus || terminalFocus"
  },
  {
    "key": "ctrl+n",
    "command": "cursorDown",
    "when": "editorTextFocus || terminalFocus"
  },
  {
    "key": "ctrl+f",
    "command": "cursorRight",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+b",
    "command": "cursorLeft",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+a",
    "command": "cursorHome",
    "when": "editorTextFocus || terminalFocus"
  },
  {
    "key": "ctrl+e",
    "command": "cursorEnd",
    "when": "editorTextFocus || terminalFocus"
  },
  {
    "key": "ctrl+d",
    "command": "deleteRight",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+h",
    "command": "deleteLeft",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+k",
    "command": "deleteAllRight",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+m",
    "command": "type",
    "args": {
      "text": "\n"
    },
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen "
  },
  {
    "key": "; d",
    "command": "workbench.view.explorer",
    "when": "neovim.mode != 'insert' && editorFocus && !explorerViewletVisible"
  },
  {
    "key": "; d",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "neovim.mode != 'insert' && explorerViewletVisible"
  },
  {
    "key": "; f",
    "command": "workbench.action.quickOpen",
    "when": "neovim.mode != 'insert' && editorFocus"
  },
  {
    "key": "; g",
    "command": "workbench.view.search",
    "when": "neovim.mode != 'insert' && editorTextFocus && !searchInputBoxFocus"
  },
  {
    "key": "; g",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "neovim.mode != 'insert' && searchInputBoxFocus"
  },
  // {
  //   "key": "[Escape]",
  //   "command": "workbench.action.navigateBack",
  //   "when": "explorerViewletVisible || searchInputBoxFocus"
  // },
  {
    "key": "ctrl+n",
    "command": "runCommands",
    "args": {
      "commands": [
        "search.action.focusNextSearchResult",
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    "when": "explorerViewletVisible || searchInputBoxFocus"
  },
  {
    "key": "ctrl+p",
    "command": "runCommands",
    "args": {
      "commands": [
        "search.action.focusNextSearchResult",
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    "when": "explorerViewletVisible || searchInputBoxFocus"
  },
  {
    "key": "n",
    "command": "explorer.newFile",
    "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
  },
  {
    "key": "f",
    "command": "explorer.newFolder",
    "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
  },
  {
    "key": "e",
    "command": "revealFileInOS",
    "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
  },
  {
    "key": "r",
    "command": "renameFile",
    "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
  },
  {
    "key": "d",
    "command": "deleteFile",
    "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
  },
  // {
  //   "key": "space",
  //   "command": "vspacecode.space",
  //   "when": "activeEditorGroupEmpty && focusedView == '' && !whichkeyActive && !inputFocus"
  // },
  // {
  //   "key": "space",
  //   "command": "vspacecode.space",
  //   "when": "sideBarFocus && !inputFocus && !whichkeyActive"
  // },
  {
    "key": "ctrl+k",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+j",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+k",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+l",
    "command": "acceptSelectedSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+j",
    "command": "showNextParameterHint",
    "when": "editorFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
  },
  {
    "key": "ctrl+k",
    "command": "showPrevParameterHint",
    "when": "editorFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
  },
  {
    "key": "ctrl+h",
    "command": "file-browser.stepOut",
    "when": "inFileBrowser"
  },
  {
    "key": "ctrl+l",
    "command": "file-browser.stepIn",
    "when": "inFileBrowser"
  },
  {
    "key": "shift+k",
    "command": "editor.action.showHover",
    "when": "editorFocus && neovim.mode != 'insert'"
  },
  {
    "key": "g r",
    "command": "editor.action.goToReferences",
    "when": "editorFocus && neovim.mode != 'insert'"
  },
  {
    "key": "g i",
    "command": "editor.action.goToImplementation",
    "when": "editorFocus && neovim.mode != 'insert'"
  },
  {
    "key": "g d",
    "command": "editor.action.revealDefinition",
    "when": "editorFocus && neovim.mode != 'insert'"
  },
  {
    "key": "g t",
    "command": "editor.action.goToTypeDefinition",
    "when": "editorFocus && neovim.mode != 'insert'"
  },
  {
    "key": "g s",
    "command": "editor.action.goToSymbol",
    "when": "editorFocus && neovim.mode != 'insert'"
  },
  {
    "key": "g n",
    "command": "editor.action.marker.nextInFiles",
    "when": "editorFocus && neovim.mode != 'insert'"
  },
  {
    "key": "g N",
    "command": "editor.action.marker.prevInFiles",
    "when": "editorFocus && neovim.mode != 'insert'"
  },
  {
    "key": "g p",
    "command": "editor.action.peekDefinition",
    "when": "editorFocus && neovim.mode != 'insert'"
  },
  {
    // I don't know why, but the default `gg` command does not take effect
    "key": "g g",
    "command": "cursorTop",
    "when": "editorFocus && neovim.mode != 'insert'"
  },
  {
    "key": "ctrl+w w",
    "command": "workbench.action.navigateEditorGroups",
    "when": "neovim.mode != 'insert' && editorFocus"
  },
  {
    "key": "ctrl+w j",
    "command": "workbench.action.navigateDown",
    "when": "neovim.mode != 'insert' && editorFocus"
  },
  {
    "key": "ctrl+w k",
    "command": "workbench.action.navigateUp",
    "when": "neovim.mode != 'insert' && editorFocus"
  },
  {
    "key": "ctrl+w h",
    "command": "workbench.action.navigateLeft",
    "when": "neovim.mode != 'insert' && editorFocus"
  },
  {
    "key": "ctrl+w l",
    "command": "workbench.action.navigateRight",
    "when": "neovim.mode != 'insert' && editorFocus"
  },
  {
    "key": "tab [",
    "command": "workbench.action.previousEditor",
    "when": "neovim.mode != 'insert' && editorFocus"
  },
  {
    "key": "tab ]",
    "command": "workbench.action.nextEditor",
    "when": "neovim.mode != 'insert' && editorFocus"
  }
]
